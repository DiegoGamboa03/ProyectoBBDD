<Window x:Class="Proyecto_base_de_datos.pages.FailedSequenceWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Proyecto_base_de_datos.pages"
        mc:Ignorable="d"
        Title="FailedSequenceWindow" Height="200" Width="300">
    <Grid>
        <Button x:Name="AcceptButton" Content="Aceptar" HorizontalAlignment="Center" Margin="0,140,0,0" VerticalAlignment="Top" Width="120" Click="Button_Click" FontSize="18" FontFamily="Segoe UI Semibold" Background="#FF0C907D" Foreground="#FFF2F4B2"/>
        <TextBlock x:Name="ErrorText" HorizontalAlignment="Center" Margin="0,24,0,0" TextWrapping="Wrap" Text="TextBlock" VerticalAlignment="Top" Height="100" Width="216" FontSize="14"/>

    </Grid>
</Window>
